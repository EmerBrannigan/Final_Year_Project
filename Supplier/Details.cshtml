@model TullymurrySystem.Data.Models.Supplier

@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>

<div>
    <h4>Supplier</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-3">Id</dt>
        <dd class="col-sm-9">@Model.Id</dd>

        <dt class="col-sm-3">Supplier Name</dt>
        <dd class="col-sm-9">@Model.SupplierName</dd>

        <dt class="col-sm-3">Telephone Number</dt>
        <dd class="col-sm-9">@Model.TelNum</dd>
    </dl>
</div>
<div class="mt-4 mb-4">
    <a asp-action="Edit" asp-route-id=@Model.Id class="btn btn-secondary btn-sm">Edit</a>
    <a asp-action="Index" class="btn btn-link">Supplier List</a>
</div>
